'scopeName': 'source.jbase'
'name': 'jBase'
'fileTypes': ['b']

'patterns': [
  {
    'begin': '\''
    'end': '\''
    'name': 'string.quoted.single.jbase'
  },
  {
    'begin': '\"'
    'end': '\"'
    'name': 'string.quoted.double.jbase'
  },
  {
    'begin': '^\\s*\\*'
    'end': '\\n'
    'name': 'comment.line.jbase'
  },
  {
    'begin': ';\\s*\\*'
    'end': '\\n'
    'name': 'comment.line.jbase'
  },
  {
    'match': '\\b(([0-9]+\\.?[0-9]*)|(\\.[0-9]+))\\b'
    'name': 'constant.numeric.jbase'
  },
  {
    'match': '@AM|@VM|@SVM'
    'name': 'constant.attributemark.jbase'
  },
  {
    'match': '''(?x)\\b(?<!\\.)
